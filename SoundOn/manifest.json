{
    "name": "SoundnOn",
    "version": "1.0",
    "description": "SoundnOn is a simple extension that plays a sound when you open a new page or tab in the browser.",
    "manifest_version": 2,
    "permissions": [
      "storage",
      "tabs",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}