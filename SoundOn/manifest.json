{
    "name": "SoundnOn",
    "version": "1.0",
    "description": "SoundnOn is a simple extension that plays a sound when you open a new page on browser. ",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "icon": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}